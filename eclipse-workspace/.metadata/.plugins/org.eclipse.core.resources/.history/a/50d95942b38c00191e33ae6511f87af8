import java.util.*;
public class Example4 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		System.out.println("Enter a value to x");
		int x = input.nextInt();
		
		if (x%2 == 0)
			System.out.println("HiEven");
		else
			System.out.println("HiOdd");
		
		input.close();
	}

}
