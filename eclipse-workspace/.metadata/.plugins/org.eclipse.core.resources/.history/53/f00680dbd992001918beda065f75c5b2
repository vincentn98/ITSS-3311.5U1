import java.util.Scanner;

public class Project2 {

	public static void main(String[] args) {
		Scanner input = new Scanner (System.in);
		
		System.out.print("Enter two characters: ");
		String s = input.nextLine();

		if (s.charAt(0) == 'b' && s.charAt(1) == '1')
			System.out.println("Biology\n to \n Java\nProgramming!");
		else if (s.charAt(0) == 'c')
			System.out.println("Computer Science");
		else if (s.charAt(0) == 'i')
			System.out.println("Information Technology and Systems");
		else if (s.charAt(0) == 'm')
			System.out.println("Marketing");
		else if (s.charAt(0) == 'h')
			System.out.println("Healthcare Management");
		else if (s.charAt(0) == 'a')
			System.out.println("Accounting");
		//else 
			//System.out.println("Invalid Major");
		else if (s.charAt(1) == '1')
			System.out.println("Freshman");
		else if (s.charAt(1) == '2')
			System.out.println("Sophmore");
		else if (s.charAt(1) == '3')
			System.out.println("Junior");
		else if (s.charAt(1) == '4')
			System.out.println("Senior");
		
		//String message = "Welcome to Java";
		//System.out.println("The first character in message is "+message.charAt(0));

		//System.out.print("Enter three words separated by spaces: ");
		//String s1 = input.next() ;
		//String s2 = input.next() ;
		//String s3 = input.next() ;
		//System.out.println("s1 is " + s1);
		//System.out.println("s2 is " + s2);
		//System.out.println("s3 is " + s3);
	}

}
